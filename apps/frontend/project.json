{
  "name": "frontend",
  "sourceRoot": "apps/frontend/src",
  "projectType": "application",
  "targets": {
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vite"
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc && vite build"
      }
    },
    "preview": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vite preview"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0"
      }
    }
  },
  "tags": []
}
